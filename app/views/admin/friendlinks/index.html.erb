<%#= render "admin/blogs/header" %>

<p id="notice"><%= notice %></p>

<h1>Friendlinks</h1>

<table>
  <thead>
    <tr>
      <th>Subject</th>
      <th>Picture</th>
      <th>Name</th>
      <th>Url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @friendlinks.each do |friendlink| %>
      <tr>
        <td><%= friendlink.subject %></td>
        <td><%= friendlink.picture %></td>
        <td><%= friendlink.name %></td>
        <td><%= friendlink.url %></td>
        <td><%= link_to 'Show', friendlink %></td>
        <td><%= link_to 'Edit', edit_friendlink_path(friendlink) %></td>
        <td><%= link_to 'Destroy', friendlink, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Friendlink', new_friendlink_path %>
